@page "/reader"

<h3>Reader || Authenticated</h3>

@(FunctionCallString)

@code
{
    [Inject]
    public HttpClient Client { get; set; }
    public string FunctionCallString { get; set; } = "";

    protected override async Task OnInitializedAsync()
    {
        FunctionCallString = await Client.GetStringAsync("/reader");
    }
}